@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ssso: <http://purl.org/ontology/ssso#> .
@prefix crm: <http://purl.org/NET/cidoc-crm/core#> .
@prefix dctype: <http://purl.org/dc/dcmitype/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dul: <http://www.loa-cnr.it/ontologies/DUL.owl#> .
@prefix event: <http://purl.org/ontology/c4dm/event.owl#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix gr: <http://purl.org/goodrelations/v1#> .
@prefix lode: <http://linkedevents.org/ontology/> .
@prefix ncal: <http://www.semanticdesktop.org/ontologies/2007/04/02/ncal#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix tio: <http://purl.org/tio/ns#> .
@prefix vann: <http://purl.org/vocab/vann/> .

<http://purl.org/ontology/ssso>
    vann:preferredNamespacePrefix "ssso" ;
    a owl:Ontology ;
    rdfs:label "SSSO", "Simple Service Status Ontology" .

ssso:ExecutedService
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "ExecutedService" ;
    rdfs:subClassOf ssso:ServiceEvent ;
    owl:disjointWith ssso:PreparedService, ssso:ProvidedService, ssso:RejectedService, ssso:ReservedService .

ssso:PreparedService
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "ReservedService" ;
    rdfs:subClassOf ssso:ServiceEvent ;
    owl:disjointWith ssso:ExecutedService, ssso:ProvidedService, ssso:RejectedService, ssso:ReservedService .

ssso:RejectedService
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "RejectedService" ;
    rdfs:subClassOf ssso:ServiceEvent ;
    owl:disjointWith ssso:ExecutedService, ssso:PreparedService, ssso:ProvidedService, ssso:ReservedService .

ssso:ReservedService
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "ReservedService" ;
    rdfs:subClassOf ssso:ServiceEvent ;
    owl:disjointWith ssso:ExecutedService, ssso:PreparedService, ssso:ProvidedService, ssso:RejectedService, ssso:ReservedService .

ssso:ServiceConsumer
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "ServiceConsumer" .

ssso:ServiceEvent
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "ServiceEvent" ;
    rdfs:subClassOf lode:Event, crm:E7_Activity, dctype:Event, gr:Individual, event:Event, tio:Event, schema:Event, schema:IndividualProduct, dul:Event, ncal:Event, prov:Activity .

ssso:ServiceFulfillment
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "ServiceFulfillment" ;
    rdfs:subClassOf ssso:ServiceEvent .

ssso:ServiceProvider
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "ServiceProvider" .

ssso:TimeTravel
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "TimeTravel" .

ssso:consumedBy
    a owl:ObjectProperty ;
    rdfs:domain ssso:ServiceEvent ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "consumedBy" ;
    rdfs:range ssso:ServiceConsumer ;
    owl:inverseOf ssso:consumes .

ssso:consumes
    a owl:ObjectProperty ;
    rdfs:domain ssso:ServiceConsumer ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "consumes" ;
    rdfs:range ssso:ServiceEvent ;
    owl:inverseOf ssso:consumedBy .

ssso:nextService
    a owl:ObjectProperty ;
    rdfs:domain ssso:ServiceEvent ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "nextService" ;
    rdfs:range ssso:ServiceEvent ;
    owl:inverseOf ssso:previousService .

ssso:previousService
    a owl:ObjectProperty ;
    rdfs:domain ssso:ServiceEvent ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "previousService" ;
    rdfs:range ssso:ServiceEvent ;
    owl:inverseOf ssso:nextService .

ssso:providedBy
    a owl:ObjectProperty ;
    rdfs:domain ssso:ServiceEvent ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "providedBy" ;
    rdfs:range ssso:ServiceProvider ;
    owl:inverseOf ssso:provides .

ssso:provides
    a owl:ObjectProperty ;
    rdfs:domain ssso:ServiceProvider ;
    rdfs:isDefinedBy <http://purl.org/ontology/ssso> ;
    rdfs:label "provides" ;
    rdfs:range ssso:ServiceEvent ;
    owl:inverseOf ssso:providedBy .

